@model DataAccess.Entity.Contact
@{
    ViewData["Title"] = "ContactUpdate";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="main-grid">
    <div class="agile-grids">
        <!-- input-forms -->
        <div class="grids">
            <div class="progressbar-heading grids-heading">
                <h2>Contact Update</h2>
            </div>
            <div class="panel panel-widget forms-panel">
                <div class="forms">
                    <div class="form-grids widget-shadow" data-example-id="basic-forms">
                        <div class="form-body">
                            <form method="post">
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Telephone</label>
                                    <input type="text" asp-for="Telephone" class="form-control" id="exampleInputEmail1">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">E-Mail</label>
                                    <input type="text" asp-for="Email" class="form-control" id="exampleInputEmail1">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Address</label>
                                    <input type="text" asp-for="Address" class="form-control" id="exampleInputEmail1">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Facebook URL</label>
                                    <input type="text" asp-for="FacebookURL" class="form-control" id="exampleInputEmail1">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Instagram URL</label>
                                    <input type="text" asp-for="InstagramURL" class="form-control" id="exampleInputEmail1">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Github URL</label>
                                    <input type="text" asp-for="GithubURL" class="form-control" id="exampleInputEmail1">
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Map URL</label>
                                    <textarea asp-for="TwitterURL" cols="30" rows="10"></textarea>
                                </div>
                                <button type="submit" class="btn btn-default w3ls-button">Update</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- //input-forms -->
    </div>
</div>

